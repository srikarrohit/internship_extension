{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
	"background": {
    "scripts": ["popup.js"]
//		"page": "popup.html"
  },
  "permissions": [
		"storage",
    "activeTab",
    "https://ajax.googleapis.com/",
		"http://*/*", 
    "https://*/*"
  ]
}
